<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" xmlns="http://javafx.com/javafx/8"
            fx:controller="ua.edu.nau.godex.projectharchenko.editor.controllers.image_preview_C">
    <center>
        <ScrollPane fx:id="SP_IP" BorderPane.alignment="CENTER">
            <content>
                <AnchorPane fx:id="AP_IP">
                    <children>
                        <ImageView fx:id="IV_IP" pickOnBounds="true"
                                   preserveRatio="true"/>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </center>
</BorderPane>
